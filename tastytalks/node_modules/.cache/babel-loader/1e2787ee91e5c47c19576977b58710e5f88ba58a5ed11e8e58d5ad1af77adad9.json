{"ast":null,"code":"import { ref, onMounted } from 'vue';\nimport { onAuthStateChanged } from 'firebase/auth';\nimport { auth } from '../firebase/index'; // Adjust the path to your Firebase configuration\nimport SignedOutHeader from './SignedOutHeader.vue';\nimport SignedInHeader from './SignedInHeader.vue';\nexport default {\n  name: 'TastyTalksHeader',\n  components: {\n    SignedOutHeader,\n    SignedInHeader\n  },\n  setup() {\n    const isUserSignedIn = ref(false);\n    onMounted(() => {\n      onAuthStateChanged(auth, user => {\n        isUserSignedIn.value = !!user;\n      });\n    });\n    return {\n      isUserSignedIn\n    };\n  }\n};","map":{"version":3,"names":["ref","onMounted","onAuthStateChanged","auth","SignedOutHeader","SignedInHeader","name","components","setup","isUserSignedIn","user","value"],"sources":["C:\\Users\\willa\\Documents\\GitHub\\ITSC-3155\\TastyTalks\\tastytalks\\src\\components\\TastyTalksHeader.vue"],"sourcesContent":["<!-- TastyTalksHeader.vue -->\r\n<template>\r\n    <!-- ... content of the header component ... -->\r\n    <header>\r\n        <div class=\"top-header\">\r\n            <div class=\"TastyTalks-LogoDiv\">\r\n                <a href=\"/\">\r\n                    <div class=\"main-icon\">\r\n                        <img src=\"../assets/images/TastkyTalkMainIconSmall.jpg\" alt=\"Main_Icon_of_the_page\">\r\n                    </div>\r\n        \r\n                    <div class=\"main-title\">\r\n                        <h1 class=\"headerlogo-font\">Tasty Talks</h1>\r\n                    </div>\r\n                    \r\n                </a>\r\n            </div>\r\n            <div class=\"utility-icons\">\r\n            </div>\r\n        </div>\r\n\r\n        <!-- NAVBAR PLACEHOLDER   -->\r\n        <!-- (Bottom rectangle with lighter green color that has the pages link inside) -->\r\n        <div class=\"bottom-header\">\r\n            <div class=\"navbar\"> \r\n\r\n                <router-link to=\"/about\">About</router-link>\r\n                <router-link to=\"/recipefeed\">Recipes</router-link>\r\n                <router-link to=\"/questionfeed\">Questions</router-link>\r\n                <router-link to=\"/choosepost\">Create Post</router-link>\r\n            </div>\r\n        </div>\r\n        <div class=\"custom-header\">\r\n            <SignedInHeader v-if=\"isUserSignedIn\" />\r\n            <SignedOutHeader v-else />\r\n        </div>\r\n        \r\n\r\n\r\n    </header>\r\n    \r\n</template>\r\n\r\n<!-- //////////////////////////////////////////// SCRIPT ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->\r\n\r\n<script>\r\nimport { ref, onMounted } from 'vue';\r\nimport { onAuthStateChanged } from 'firebase/auth';\r\nimport { auth } from '../firebase/index'; // Adjust the path to your Firebase configuration\r\nimport SignedOutHeader from './SignedOutHeader.vue';\r\nimport SignedInHeader from './SignedInHeader.vue';\r\n\r\nexport default {\r\n    name: 'TastyTalksHeader',\r\n    components: {\r\n        SignedOutHeader,\r\n        SignedInHeader\r\n    },\r\n    setup() {\r\n        const isUserSignedIn = ref(false);\r\n\r\n        onMounted(() => {\r\n            onAuthStateChanged(auth, (user) => {\r\n                isUserSignedIn.value = !!user;\r\n            });\r\n        });\r\n\r\n        return { isUserSignedIn };\r\n    }\r\n}\r\n</script>\r\n  \r\n\r\n<!-- /////////////////////////////////////////// CSS  /////////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->\r\n\r\n\r\n<style scoped>\r\n    /* Component-specific styles go here */\r\n\r\n    .top-header {\r\n        display: flex;\r\n        /* top, right, bottom, left */\r\n        padding: 1% 5% 1% 3%;\r\n        justify-content: space-between;\r\n        border-bottom: 2px inset #373D20;\r\n    }\r\n\r\n    .main-title{\r\n        white-space: nowrap;;\r\n    }\r\n    \r\n    .main-title{\r\n        font-size: 20px;\r\n        font-family: var(--TestingFont);\r\n        color: var(--Trying2);\r\n    }\r\n\r\n    .top-header a{\r\n        display: flex;\r\n        text-decoration: none;\r\n        gap: 10px;\r\n    }\r\n    header{\r\n        display: flex;\r\n        align-items: stretch;\r\n        width: 100%;\r\n        height: 100%;\r\n        background-color: var(--Trying2);\r\n        font-family: var(--TestingFont);\r\n    }\r\n    \r\n    .bottom-header{\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n        width: 55%;\r\n        border-bottom: 2px inset #373D20;\r\n    }\r\n    .custom-header{\r\n        display: flex;\r\n        justify-content: flex-end;\r\n        align-items: center;\r\n        width: 45%;\r\n        max-height: auto;\r\n        border-bottom: 2px inset #373D20;\r\n    }\r\n\r\n\r\n</style>"],"mappings":"AA8CA,SAASA,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,SAASC,kBAAiB,QAAS,eAAe;AAClD,SAASC,IAAG,QAAS,mBAAmB,EAAE;AAC1C,OAAOC,eAAc,MAAO,uBAAuB;AACnD,OAAOC,cAAa,MAAO,sBAAsB;AAEjD,eAAe;EACXC,IAAI,EAAE,kBAAkB;EACxBC,UAAU,EAAE;IACRH,eAAe;IACfC;EACJ,CAAC;EACDG,KAAKA,CAAA,EAAG;IACJ,MAAMC,cAAa,GAAIT,GAAG,CAAC,KAAK,CAAC;IAEjCC,SAAS,CAAC,MAAM;MACZC,kBAAkB,CAACC,IAAI,EAAGO,IAAI,IAAK;QAC/BD,cAAc,CAACE,KAAI,GAAI,CAAC,CAACD,IAAI;MACjC,CAAC,CAAC;IACN,CAAC,CAAC;IAEF,OAAO;MAAED;IAAe,CAAC;EAC7B;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}